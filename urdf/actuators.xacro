    </xacro:aft_fin_macro>
    
    <xacro:aft_fin_macro namespace="${namespace}" fin_id="5" mesh_filename="${aft_fin_mesh_file}">
        <!--<origin xyz="-3.4129751 0 -0.40526605" rpy="${1.0 * pi} 0 0" />-->
        <origin xyz="-3.4129751 0 -0.40526605" rpy="${0.0 * pi} 0 0" />
    </xacro:aft_fin_macro>

</robot>
