          </liftdrag>
          <current_velocity_topic>current_velocity</current_velocity_topic>
          <fin_id>${fin_id}</fin_id>
          <link_name>${namespace}/fin${fin_id}</link_name>
          <joint_name>${namespace}/fin${fin_id}_joint</joint_name>
