  <xacro:macro name="back_down_fin_macro" params="namespace fin_id *origin mesh_filename">
    <link name="${namespace}/fin${fin_id}">
      <!--<xacro:no_inertial/>-->
      <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
